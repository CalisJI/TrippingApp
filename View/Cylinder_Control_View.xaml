                SS11_Up="{Binding SS11_Up}"
                SS11_Down="{Binding SS11_Down}"
                
                SS12_Up="{Binding SS12_Up}"
                SS12_Down="{Binding SS12_Down}"
                
                SS13_Up="{Binding SS13_Up}"
                SS13_Down="{Binding SS13_Down}"
                
                SS14_Up="{Binding SS14_Up}"
                SS14_Down="{Binding SS14_Down}"
                
