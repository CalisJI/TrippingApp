                XL9_Up_Command="{Binding XL9_Up_Command}"
                XL9_Down_Command="{Binding XL9_Down_Command}"
                XL10_Up_Command="{Binding XL10_Up_Command}"
                XL10_Down_Command="{Binding XL10_Down_Command}"
                XL11_Up_Command="{Binding XL11_Up_Command}"
                XL11_Down_Command="{Binding XL11_Down_Command}"
                XL12_Up_Command="{Binding XL12_Up_Command}"
                XL12_Down_Command="{Binding XL12_Down_Command}"
                XL13_Up_Command="{Binding XL13_Up_Command}"
                XL13_Down_Command="{Binding XL13_Down_Command}"
                XL14_Up_Command="{Binding XL14_Up_Command}"
                XL14_Down_Command="{Binding XL14_Down_Command}"
                
                
                SS1_Up="{Binding SS1_Up}"
                SS1_Down="{Binding SS1_Down}"
                
                SS2_Up="{Binding SS2_Up}"
                SS2_Down="{Binding SS2_Down}"
                
                SS3_Up="{Binding SS3_Up}"
