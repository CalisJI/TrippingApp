    <Viewbox>
        <Grid Width="1920" Height="1080">
            <Grid.ColumnDefinitions>
                <ColumnDefinition/>
                <ColumnDefinition/>
                <ColumnDefinition/>
                <ColumnDefinition/>
                <ColumnDefinition/>
                <ColumnDefinition/>
            </Grid.ColumnDefinitions>
            <Grid.RowDefinitions>
                <RowDefinition/>
                <RowDefinition/>
                <RowDefinition/>
                <RowDefinition/>
            </Grid.RowDefinitions>

            <!--Simulation Process Page-->
            <Button
                    Command="{Binding Simulation_Page_Command}"
                    themes:ButtonAssist.CornerRadius="25"
                    Width="280"
                    Height="210"
                    VerticalAlignment="Center"
                    Grid.Column="1"
                    Grid.Row="1">
                <themes:PackIcon
                        Height="180"
                        Width="180"
                        Kind="CogTransfer"/>


            </Button>
            <!--Process Param Page-->
            <Button
                Command="{Binding Process_Param_Page}"
                themes:ButtonAssist.CornerRadius="25"
                Width="280"
                Height="210"
                VerticalAlignment="Center"                    
                Grid.Column="2"
                Grid.Row="1">
                <themes:PackIcon
                    Height="180"
                    Width="180"
                    Kind="MicrosoftExcel"/>
            </Button>
            <!--Setting Page-->
            <Button
                Command="{Binding Setting_Page_Command}"
                themes:ButtonAssist.CornerRadius="25"
                Width="280"
                Height="210"
                VerticalAlignment="Center"                    
                Grid.Column="3"
                Grid.Row="1">
                <themes:PackIcon
                        Height="180"
                        Width="180"
                        Kind="ApplicationBrackets"/>
            </Button>
            <!--Dashboard Page-->
            <Button
                    themes:ButtonAssist.CornerRadius="25"
                    Width="280"
                    Height="210"
                VerticalAlignment="Center"                    
                    Grid.Column="1"
                    Grid.Row="2">
                <Image
                    Margin="10"
                    Source="{StaticResource monitor_analytics_svgrepo_comDrawingImage}"/>
            </Button>
            <!--Data Machine Page-->
            <Button
                    themes:ButtonAssist.CornerRadius="25"
                    Width="280"
                    Height="210"
                VerticalAlignment="Center"                    
                    Grid.Column="2"
                    Grid.Row="2">
                <themes:PackIcon
                        Height="180"
                        Width="180"
                        Kind="Atom"/>
            </Button>
            <!--Robot And Manual Configuration Page-->
            <Button
                Command="{Binding Robot_And_Manual_Configuration_Page_Command}"
                themes:ButtonAssist.CornerRadius="25"
                Width="280"
                Height="210"
                VerticalAlignment="Center"                    
                Grid.Column="3"
                Grid.Row="2">
                <Image
                    Margin="10"
                    Source="{StaticResource settings_svgrepo_comDrawingImage}"/>
            </Button>
            <!--Camera API Page-->
            <Button
                Command="{Binding CameraAPI_Page_Command}"
                themes:ButtonAssist.CornerRadius="25"
                Width="280"
                Height="210"
                VerticalAlignment="Center"                    
                Grid.Column="4"
                Grid.Row="1">
                <Image
                    Margin="10"
                    Source="{StaticResource camera_svgrepo_comDrawingImage}"/>
            </Button>
        </Grid>
    </Viewbox>
    
</UserControl>
